import * from "atom:std";

async func wast() {
    std.println("wait->start");
    return "Hola!";
}

async func callee() {
    std.println("callee->start");
    return await wast();
}

async func caller() {
    std.println("Starting...");
    const data = await callee();
    std.println("caller->callee() =", data);
    return data;
}

caller();

