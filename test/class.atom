import "atom:std";
import "gui";
import "hash";
import "./imports.atom";

class Animal {
    func init(self, name) {
        self.name = name;
    }

    // Common property for an animals
    func eat(self, food) {
        std.println(self.name + " is eating " + food);
    }

}

func reg() {
    (3 / 0) catch (err) {
        std.println(">>>", err);
        // std.throw(err);
    };;
    return 2;
}

async func waitable() {
    return await reg() + 23;
}

class Dog extends Animal {
    func init(self, name, ageInMonths) {
        self.name = name;
        self.age = ageInMonths;
    }

    async func process(self) {
        return waitable();
    }
}

const dog = new Dog("Buddy", 12);
std.println(dog);

const d = dog.eat("bone");

std.println(">>", d, dog.process());

